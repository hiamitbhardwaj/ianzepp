        <precision>18</precision>
        <scale>2</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>PointsValue__c</fullName>
        <formula>IF (OR (TEXT(Points__c) = &apos;?&apos;, TEXT(Points__c) = &apos;∞&apos;), NULL, VALUE (TEXT(Points__c)))</formula>
