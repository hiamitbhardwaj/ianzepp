<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <deploymentStatus>Deployed</deploymentStatus>
    <enableHistory>true</enableHistory>
    <fields>
        <fullName>CompletedOn__c</fullName>
        <label>Completed On</label>
        <trackHistory>true</trackHistory>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>DaysInProgress__c</fullName>
        <formula>IF (ISNULL (CompletedOn__c) &amp;&amp; ISNULL (StartedOn__c), 0,
IF (ISNULL (CompletedOn__c), TODAY () - StartedOn__c, CompletedOn__c - StartedOn__c))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days In Progress</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>DaysOverdue__c</fullName>
        <formula>IF (ISNULL (DueOn__c) = FALSE, MAX (0, TODAY () - DueOn__c), 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days Overdue</label>
        <precision>18</precision>
        <scale>0</scale>
        <type>Number</type>
    </fields>
